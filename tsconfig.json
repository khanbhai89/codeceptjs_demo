{
  "ts-node": {
    "files": true,
    "transpileOnly": true
  },
  "compilerOptions": {
    "target": "es2019",
    "lib": [
      "es2019",
      "DOM",
      "DOM.Iterable"
    ],
    "esModuleInterop": true,
    "module": "commonjs",
    "strictNullChecks": true,
    "noImplicitAny": true,
    "types": [
      "codeceptjs",
      "node"
    ],
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  }
}